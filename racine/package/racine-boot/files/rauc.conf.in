[system]
compatible=@RACINE_BOOT_RAUC_COMPATIBLE@
bootloader=uboot
bundle-formats=-plain
statusfile=/data/central-status.raucs

[keyring]
path=ca.pem

[slot.rootfs.0]
device=/dev/disk/by-partlabel/@RACINE_BOOT_ROOTFS_A_PART_LABEL@
type=ext4
bootname=A

[slot.rootfs.1]
device=/dev/disk/by-partlabel/@RACINE_BOOT_ROOTFS_B_PART_LABEL@
type=ext4
bootname=B
